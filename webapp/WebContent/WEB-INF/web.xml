<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>webapp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <context-param>
  <param-name>company-name</param-name>
  <param-value>Tyss</param-value>
  </context-param>
  
  <servlet>
  <servlet-name>firstServlet</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.MyFirstServelet</servlet-class>
  <init-param>
  <param-name>Batch-name</param-name>
  <param-value>UST-GLOBAL</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>firstServlet</servlet-name>
<url-pattern>/first</url-pattern>
  
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>formServlet</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.FormServlet</servlet-class>
  
   <init-param>
  <param-name>Batch-name</param-name>
  <param-value>TY</param-value>
  </init-param>
  
  </servlet>
  <servlet-mapping>
  <servlet-name>formServlet</servlet-name>
<url-pattern>/form</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>redirect</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.RedirectServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>redirect</servlet-name>
<url-pattern>/redirect</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>forward</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.ForwardServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>forward</servlet-name>
<url-pattern>/forward</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>include</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.IncludeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>include</servlet-name>
<url-pattern>/include</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>formForward</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.FormForwardServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>formForward</servlet-name>
<url-pattern>/formForward</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
  <servlet-name>createCookie</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.CreateCookieServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>createCookie</servlet-name>
<url-pattern>/create-cookie</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
  <servlet-name>readCookie</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.ReadCookieServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>readCookie</servlet-name>
<url-pattern>/read-cookie</url-pattern>
  </servlet-mapping>
  
  
  
   <servlet>
  <servlet-name>contextAttribute</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.SetContextAttributeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>contextAttribute</servlet-name>
<url-pattern>/set-context</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  <servlet-name>getcontextAttribute</servlet-name>
  <servlet-class>com.ustglobal.wepapp.servelets.GetContextAttributeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>getcontextAttribute</servlet-name>
<url-pattern>/get-context</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>